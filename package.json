{
  "name": "polyverse-sara-web-ui",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev-docker": "docker-compose up -d && cross-env KV_REST_API_URL='http://localhost:8079' KV_REST_API_TOKEN='example_token' NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build-test": "tsc && ./test/add-test-build-types.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "test": "pnpm run build-test && node --loader ts-node/esm.mjs node_modules/mocha/lib/cli/cli.js 'dist/esm/**/*.spec.js'",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky install"
  },
